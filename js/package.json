{
  "name": "jupyter-materialui",
  "version": "0.1.0",
  "description": "Jupyter Widgets based on React Material UI components",
  "author": "Maarten A. Breddels",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/maartenbreddels/ipymaterialui.git"
  },
  "keywords": [
    "jupyter",
    "widgets",
    "ipython",
    "ipywidgets",
    "jupyterlab-extension"
  ],
  "files": [
    "lib/**/*.js",
    "dist/*.js"
  ],
  "scripts": {
    "clean": "rimraf dist/",
    "prepare": "npm run build:lib && webpack",
    "build:lib": "babel src --out-dir lib",
    "prepublish": "webpack",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "css-loader": "^1.0.1",
    "rimraf": "^2.6.1",
    "style-loader": "^0.23.1",
    "webpack": "^4.26.0",
    "webpack-cli": "^3.1.2"
  },
  "dependencies": {
    "@jupyter-widgets/base": "^1.0.0",
    "@material-ui/core": "^3.4.1",
    "@material-ui/icons": "^3.0.1",
    "@material-ui/lab": "^3.0.0-alpha.23",
    "lodash": "^4.17.4",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "typeface-roboto": "0.0.54"
  },
  "jupyterlab": {
    "extension": "lib/labplugin"
  }
}
