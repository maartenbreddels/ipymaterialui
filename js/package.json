{
  "name": "jupyter-materialui",
  "version": "0.1.4",
  "description": "Jupyter Widgets based on React Material UI components",
  "license": "MIT",
  "author": "Maarten A. Breddels, Mario Buikhuizen",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/maartenbreddels/ipymaterialui.git"
  },
  "keywords": [
    "jupyter",
    "widgets",
    "ipython",
    "ipywidgets",
    "jupyterlab-extension"
  ],
  "files": [
    "lib/",
    "src/",
    "dist/"
  ],
  "browserslist": ">0.8%, not ie 11, not op_mini all, not dead",
  "scripts": {
    "clean": "rimraf lib/ dist/",
    "build:lib": "babel src --out-dir lib --copy-files",
    "watch:lib": "babel src --out-dir lib --copy-files --watch --verbose",
    "build:bundle": "webpack",
    "watch:bundle": "webpack --mode development --watch",
    "prepare": "run-s build:*",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "run-p watch:*",
    "lint": "eslint src/**/*.js src/**/*.jsx"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "css-loader": "^1.0.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "file-loader": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.3",
    "style-loader": "^0.23.1",
    "webpack": "^4.37.0",
    "webpack-cli": "^3.3.6"
  },
  "dependencies": {
    "@jupyter-widgets/base": "^1.0.0 || ^2.0.0 || ^3.0.0",
    "@material-ui/core": "^4.2.1",
    "@material-ui/lab": "^4.0.0-alpha.20",
    "core-js": "^3.1.4",
    "lodash": "^4.17.15",
    "material-design-icons-iconfont": "^5.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "typeface-roboto": "0.0.54"
  },
  "jupyterlab": {
    "extension": "lib/labplugin"
  }
}
